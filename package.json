{
  "name": "react-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:build:client": "cross-env NODE_ENV=development webpack --config build/webpack.client.config.js --watch",
    "dev:build:server": "cross-env NODE_ENV=development webpack --config build/webpack.server.config.js --watch",
    "dev:server": "nodemon --watch dist --exec node \"./dist/bundle.js\"",
    "dev": "concurrently \"yarn:dev:*\"",
    "prod:build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js",
    "prod:build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js",
    "prod": "yarn prod:build:client && yarn prod:build:server && node ./dist/bundle.js",
    "start:dev": "concurrently \"yarn dev\" \"yarn server:api\"",
    "start:prod": "concurrently \"yarn prod\" \"yarn server:api\"",
    "server:api": "nodemon api-server/app.js"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-http-proxy": "^1.5.1",
    "http-proxy-middleware": "^0.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.0",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.7.1",
    "babel-loader": "^8.0.4",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.1.0",
    "isomorphic-style-loader": "^5.1.0",
    "nodemon": "^1.19.1",
    "postcss-loader": "^3.0.0",
    "sass": "^1.23.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
